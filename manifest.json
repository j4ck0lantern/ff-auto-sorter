{
  "manifest_version": 3,
  "name": "Auto Sorter",
  "version": "2.0",
  "description": "Automatically organizes new and existing bookmarks into hobby folders based on keywords in the title and URL.",
  "host_permissions": [
    "*://generativelanguage.googleapis.com/*",
    "*://api.openai.com/*",
    "*://*/*"
  ],
  "permissions": [
    "bookmarks",
    "storage",
    "menus",
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": [
      "db.js",
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "autosorter@example.com",
      "strict_min_version": "115.0",
      "data_collection_permissions": false
    }
  }
}